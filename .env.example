# Copy this file to .env and fill in your actual credentials
# NEVER commit .env to version control

# MySQL Database
MYSQL_HOST=your-mysql-host.rds.amazonaws.com
MYSQL_PORT=3306
MYSQL_USER=your_username
MYSQL_PASSWORD=your_password
MYSQL_DATABASE=your_database

# LLM API - Volces (primary)
VOLCES_API_KEY=your_volces_api_key
VOLCES_BASE_URL=https://ark.cn-beijing.volces.com/api/v3
VOLCES_MODEL=your_model_endpoint_id

# LLM API - OpenAI (fallback)
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4

# AWS S3
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-2
S3_BUCKET=your_bucket_name
